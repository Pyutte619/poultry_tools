            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>


        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_sumProtein"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:layout_marginTop="5dp"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="20dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_sumEnergi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:layout_marginTop="5dp"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="20dip"
            flatui:fl_theme="@array/grass"/>
    </LinearLayout>
    <com.cengalabs.flatui.views.FlatButton
        android:id="@+id/button_back"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="Kembali"
        android:layout_weight="1"
        flatui:fl_textAppearance="fl_light"
        android:layout_margin="10dp"
        flatui:fl_theme="@array/grass"
        flatui:fl_blockButtonEffectHeight="3dp"
        android:layout_below="@+id/linearLayout"
        android:layout_centerHorizontal="true" />
</RelativeLayout>