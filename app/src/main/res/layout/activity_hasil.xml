<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:flatui="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/title_Hasil"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dip"
        android:maxLines="1"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="@color/grass_primary"
        android:fontFamily="roboto"
        android:text="Hasil Protein"
        android:textSize="20dip"
        android:shadowColor="@color/grass_dark"
        />
    <LinearLayout
        android:layout_width="200dp"
        android:orientation="vertical"
        android:layout_below="@+id/title_Hasil"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout">

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Pakan Jadi"
            android:textSize="16dip"
            flatui:backgroundTint="@color/grass_light"
            flatui:fl_theme="@array/grass"
             />

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Bekatul Padi"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Dedak Padi"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Jagung Giling"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Polar"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Tepung Ikan 1"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Tepung Ikan 2"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Konsentrat"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Bungkil Kedele"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_txt10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Protein Minyak Sawit"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_sumProtein"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:layout_marginTop="5dp"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Jumlah Protein"
            android:textSize="20dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/title_sumEnergi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:layout_marginTop="5dp"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="Jumlah Energi"
            android:textSize="20dip"
            flatui:fl_theme="@array/grass"/>
        </LinearLayout>

    <LinearLayout
        android:layout_width="150dp"
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/linearLayout"
        android:layout_toRightOf="@+id/linearLayout"
        android:layout_toEndOf="@+id/linearLayout">

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:backgroundTint="@color/grass_light"
            flatui:fl_theme="@array/grass"
            />

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dip"
            android:maxLines="1"
            android:gravity="center_vertical"
            android:text="0"
            android:textSize="16dip"
            flatui:fl_theme="@array/grass"/>

        <com.cengalabs.flatui.views.FlatTextView
            android:id="@+id/text_hasil4"
